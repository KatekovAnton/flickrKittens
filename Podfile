# Uncomment this line to define a global platform for your project
platform :ios, '8.0'



def import_pods
   pod 'AFNetworking'
   pod 'AFNetworkActivityLogger', :git => 'https://github.com/AFNetworking/AFNetworkActivityLogger.git', :branch => '3_0_0'
   pod 'EasyMapping'
   pod 'PureLayout'
end



target 'flickrBrowser' do
    import_pods
end
